<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
    <link rel="stylesheet" href="/stylesheets/index.css">
    {{>headComponent}}
  </head>
  <body>
    {{>navbarComponent}}
    
    <!-- FIXME aria labeledby -->
    <div class="modal fade in" tabindex="-1" role="dialog" aria-labelledby="gridSystemModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <!-- FIXME aria label -->
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
            <!-- FIXME id -->
            <h4 class="modal-title" id="gridSystemModalLabel">{{welcomeHeader}}</h4>
          </div>
          <div class="modal-body">
            <div class="row">
              <div class="col-sm-offset-1 col-sm-10"><p>{{welcomeText}}</p></div>
            </div>
            <div class="modal-footer">
              <div class="row">
                <button type="button" class="btn btn-default col-sm-offset-1 col-sm-3" data-dismiss="modal">{{closeText}}</button>
                <button type="button" class="btn btn-primary col-sm-4">{{loginFormTitle}}</button>
                <button type="button" class="btn btn-primary col-sm-3">{{registerFormTitle}}</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <main class="content-container">
      
    </main>
    <script src="/scripts/index.js"></script>
  </body>
</html>
